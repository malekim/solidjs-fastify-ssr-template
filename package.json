{
  "name": "solid-js-vite-ssr-fastify",
  "version": "1.0.0",
  "description": "A template for solid-js project with SSR in Fastify",
  "author": "Lukasz Malek <dev@malek.im>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "solid",
    "solid-js",
    "ssr"
  ],
  "main": "main.ts",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "build": "yarn build:client && yarn build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "dev": "cross-env NODE_ENV=development nodemon -r dotenv/config ./main.ts",
    "start": "cross-env NODE_ENV=production tsx -r dotenv/config ./main.ts"
  },
  "dependencies": {
    "@fastify/middie": "^8.0.0",
    "@fastify/secure-session": "^5.2.0",
    "@fastify/static": "^6.5.0",
    "@mikro-orm/core": "^5.5.0",
    "@mikro-orm/mongodb": "^5.5.0",
    "@mikro-orm/reflection": "^5.5.0",
    "axios": "^1.1.3",
    "express": "^4.17.1",
    "fastify": "^4.9.2",
    "nodemailer": "^6.8.0",
    "serve-static": "^1.14.1",
    "solid-app-router": "^0.4.2",
    "solid-js": "^1.6.0",
    "solid-meta": "^0.28.1",
    "tslib": "^2.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.5",
    "@types/nodemailer": "^6.4.6",
    "@types/serve-static": "^1.13.10",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-preset-solid": "^1.1.3",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-solid": "^0.7.4",
    "jest": "^29.2.2",
    "jiti": "^1.12.0",
    "nodemon": "^2.0.13",
    "rollup-plugin-typescript-paths": "^1.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsx": "^3.11.0",
    "typescript": "*",
    "vite": "^2.5.7",
    "vite-plugin-solid": "^2.0.3"
  }
}
